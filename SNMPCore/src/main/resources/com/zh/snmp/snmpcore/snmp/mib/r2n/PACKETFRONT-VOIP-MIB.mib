-- *****************************************************************************
-- Call statistics 
--
-- *****************************************************************************
voipCallQuality               OBJECT IDENTIFIER       ::= { drgVoipObjects 11 }

voipCallQualityNotify OBJECT-TYPE
    SYNTAX INTEGER {
      disabled(1),
      always(2),
      onlyErrors(3)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Setting the object to always(2) causes the system to send
    notifyVoipCallQuality after every call compeltes
    no matter of result.

    Setting the object to onlyErrors(3) cause the system to
    send NotifyVoipCallQuality when an error occurs during
    the call.

    Setting the object to disabled(1) cause the system to
    not send any notifyVoipCallQuality."
      ::= { voipCallQuality 1 }

voipCallQualityTable OBJECT-TYPE
   SYNTAX SEQUENCE OF VoipCallQualityEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION
      "A table with information about the status of FXS line interface."
      ::= { voipCallQuality 2 }

voipCallQualityEntry OBJECT-TYPE
    SYNTAX VoipCallQualityEntry 
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "One row of the table."
    INDEX { voipCallQualityIndex }
      ::= { voipCallQualityTable 1 }

VoipCallQualityEntry ::= SEQUENCE {
    voipCallQualityIndex Integer32,
    voipCallQualityJitter Integer32,
    voipCallQualityLatency Integer32,
    voipCallQualityPacketsLost Integer32,
    voipCallQualityPacketsTotal Integer32,
    voipCallQualityDuration Integer32,
    voipCallQualityAPartyNumber OCTET STRING,
    voipCallQualityAPartyName OCTET STRING,
    voipCallQualityBPartyNumber OCTET STRING,
    voipCallQualityBPartyName OCTET STRING,
    voipCallQualityAPartyAddress IpAddress,
    voipCallQualityBPartyAddress IpAddress
}

voipCallQualityIndex OBJECT-TYPE
    SYNTAX Integer32 (1..4)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "A unique index for the call results table. Each row represents the statistics
    for the last call made on a specific line. Currently, only the statistics for
    last call can be got no matter which line does that, so only one entry is available."
      ::= { voipCallQualityEntry 1 }

voipCallQualityJitter OBJECT-TYPE
    SYNTAX Integer32
    UNITS "milliseconds"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The average RTP jitter measured during the last call"
      ::= { voipCallQualityEntry 2 }

voipCallQualityLatency OBJECT-TYPE
    SYNTAX Integer32
    UNITS "milliseconds"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The average RTP latency measured during the last call"
      ::= { voipCallQualityEntry 3 }

voipCallQualityPacketsLost OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The number of RTP frames not received in the last call. The loss could
    be caused by network loss, or dropped by the jitter buffer due to late
    arrival"
      ::= { voipCallQualityEntry 4 }

voipCallQualityPacketsTotal OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The total number of expected packets in the last call."
      ::= { voipCallQualityEntry 5 }

voipCallQualityDuration OBJECT-TYPE
    SYNTAX Integer32
    UNITS "seconds"
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The duration of the last call in seconds."
      ::= { voipCallQualityEntry 6 }

voipCallQualityAPartyNumber OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..32))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The A-party number associated with the last call. Note that if the number
    is not known, then 'UNKNOWN' will be used instead"
      ::= { voipCallQualityEntry 7 }

voipCallQualityAPartyName OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..32))  
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The A-party name associated with the last call. Note that if the name
    is not known, then 'UNKNOWN' will be used instead"
      ::= { voipCallQualityEntry 8 }

voipCallQualityBPartyNumber OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..32))
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The B-party number associated with the last call. Note that if the number
    is not known, then 'UNKNOWN' will be used instead"
      ::= { voipCallQualityEntry 9 }

voipCallQualityBPartyName OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..32))  
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The B-party name associated with the last call. Note that if the name
    is not known, then 'UNKNOWN' will be used instead"
      ::= { voipCallQualityEntry 10 }

voipCallQualityAPartyAddress OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The A-party IP address associated with the media stream. This is the
    RTP gateway address, which is not necessarily the same as the signalling
    address"
      ::= { voipCallQualityEntry 11 }

voipCallQualityBPartyAddress OBJECT-TYPE
    SYNTAX IpAddress
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The B-party IP address associated with the media stream. This is the
    RTP gateway address, which is not necessarily the same as the signalling
    address"
      ::= { voipCallQualityEntry 12 }


