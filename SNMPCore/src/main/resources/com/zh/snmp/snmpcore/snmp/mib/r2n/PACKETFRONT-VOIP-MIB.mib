voipSIPInviteTimerValue OBJECT-TYPE
    SYNTAX Integer32 (0..1000)  
    UNITS "Seconds"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	"Timer value ..., a value of 0 indicates that this option is disabled"
      ::= { voipSIPTimers 1 }

voipSIPSessionTimerValue OBJECT-TYPE
    SYNTAX Integer32 (0..1000)  
    UNITS "Seconds"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	"Timer value ..., a value of 0 indicates that this option is disabled"
      ::= { voipSIPTimers 2 }

voipSIPNotifyTimerValue OBJECT-TYPE
    SYNTAX Integer32 (0..1000)  
    UNITS "Seconds"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
	"Timer value ..., a value of 0 indicates that this option is disabled"
      ::= { voipSIPTimers 3 }

voipSIPInviteIncludeSdp OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Controls whether SIP invite messages should include SDP parameters"
      ::= { voipSIPOptions 6 }

voipSIPTelephoneURIEnabled OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Controls whether the use of the telephone-URI method is enabled"
      ::= { voipSIPOptions 7 }

voipSIPAnonymous OBJECT IDENTIFIER ::= { voipSIPOptions 8 }

voipSIPAnonymousTable OBJECT-TYPE
   SYNTAX SEQUENCE OF VoipSIPAnonymousEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION
   "Table describes the anonymous call behaviour. One row per line"
   ::= { voipSIPAnonymous 1 }

voipSIPAnonymousEntry OBJECT-TYPE
    SYNTAX VoipSIPAnonymousEntry 
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "One row of the table."
    INDEX { voipIfIndex }
      ::= { voipSIPAnonymousTable 1 }

VoipSIPAnonymousEntry ::= SEQUENCE {
    voipSIPAnonymousLineNumber Integer32,
    voipSIPAnonymousFromHeaderEnabled TruthValue,
    voipSIPAnonymousToHeaderEnabled TruthValue,
    voipSIPAnonymousProxyRequiresPrivacyEnabled TruthValue,
    voipSIPAnonymousDisplayNameEnabled TruthValue
}

voipSIPAnonymousLineNumber OBJECT-TYPE
    SYNTAX Integer32 (1..4)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "A unique value, greater than zero, for each table row. "
      ::= { voipSIPAnonymousEntry 1 }

voipSIPAnonymousFromHeaderEnabled OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Enable the use of an 'anonymous' in From and Contact headers."
      ::= { voipSIPAnonymousEntry 2 }

voipSIPAnonymousToHeaderEnabled OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Enable the use of 'anonymous' From header when transferring a call which
    is made with temporary CLIR in force."
      ::= { voipSIPAnonymousEntry 3 }

voipSIPAnonymousProxyRequiresPrivacyEnabled OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Enable the inclusion of 'privacy' in ProxyRequires header when call is
    made with temporary CLIR in force"
      ::= { voipSIPAnonymousEntry 4 }

voipSIPAnonymousDisplayNameEnabled OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Enable the use of an 'anonymous' display name when call is made with
    temporary CLIR in force, and voipSIPAnonymousFromHeaderEnable is not
    enabled."
      ::= { voipSIPAnonymousEntry 5 }


voipSIPFeatures OBJECT IDENTIFIER ::= { voipSIPOptions 9 }

voipSIPFeaturesTable OBJECT-TYPE
   SYNTAX SEQUENCE OF VoipSIPFeaturesEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION
      "A table with information about the status of FXS line interface."
   ::= { voipSIPFeatures 1 }

voipSIPFeaturesEntry OBJECT-TYPE
    SYNTAX VoipSIPFeaturesEntry 
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "One row of the table."
    INDEX { voipIfIndex }
      ::= { voipSIPFeaturesTable 1 }

VoipSIPFeaturesEntry ::= SEQUENCE {
    voipSIPFeaturesLineNumber Integer32,
    voipSIPLocalRingingEnabled TruthValue,
    voipSIPMediaDirection INTEGER,
    voipSIPSuspendTimer Integer32,
    voipSIPPayphoneReversePolarityEnabled TruthValue,
    voipSIPOutOfBandFlashMethod INTEGER,
    voipSIPOutboundProxyAddress OCTET STRING, 
    voipSIPOutboundProxyPort Integer32
}

voipSIPFeaturesLineNumber OBJECT-TYPE
    SYNTAX Integer32 (1..4)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "A unique value, greater than zero, for each table row. "
      ::= { voipSIPFeaturesEntry 1 }

voipSIPLocalRingingEnabled OBJECT-TYPE  
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Enable the support of local ringing generation when the instance receives
    a call proceeding message"
      ::= { voipSIPFeaturesEntry 2 }

voipSIPMediaDirection OBJECT-TYPE  
    SYNTAX INTEGER {
      off(1),
      active(2),
      passive(3),
      both(4),
      reconnect(5)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Controls the default value used for SDP media direction tag. This is
    useful when operating behind a NAT device"
      ::= { voipSIPFeaturesEntry 3 }

voipSIPSuspendTimer OBJECT-TYPE
    SYNTAX Integer32 (0..120)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Determines the maximum time that a call can be placed in the SUSPEND state"
      ::= { voipSIPFeaturesEntry 4 }

voipSIPPayphoneReversePolarityEnabled OBJECT-TYPE  
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Enable line battery voltage reversal on call establishment and revert to
    normal battery voltage on call completion. This feature is used by some
    payphone systems to determine when to charge for a call."
      ::= { voipSIPFeaturesEntry 5 }

voipSIPOutOfBandFlashMethod OBJECT-TYPE  
    SYNTAX INTEGER {
      disabled(1),
      sipInfo(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Determines if hookflash signals shall be sent using an out-of-band
    method, and if they should, what method is used."
      ::= { voipSIPFeaturesEntry 6 }

voipSIPOutboundProxyAddress OBJECT-TYPE 
    SYNTAX OCTET STRING (SIZE(0..255))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Comma delimited list of IP addresses and/or FQDNs for the outbound proxy 
    server."
      ::= { voipSIPFeaturesEntry 7 }

voipSIPOutboundProxyPort OBJECT-TYPE
    SYNTAX Integer32 (1024..65535)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Comma delimited list of destination ports used when communicating with an 
    outbound proxy server."
      ::= { voipSIPFeaturesEntry 8 }

-- *****************************************************************************
-- H323 signalling options
--
-- *****************************************************************************

voipH323ProtocolOptions OBJECT IDENTIFIER ::= { voipH323Options 1 }
voipH323GatekeeperOptions OBJECT IDENTIFIER ::= { voipH323Options 2 }
voipH323SecurityOptions OBJECT IDENTIFIER ::= { voipH323Options 3 }
voipH323MessageWaitingOptions OBJECT IDENTIFIER ::= { voipH323Options 4 }
voipH323CallParkingOptions OBJECT IDENTIFIER ::= { voipH323Options 5 }


-- Protocol options
voipH323SupportFastConnect OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Support the Fast Connect method..."
      ::= { voipH323ProtocolOptions 1 }

voipH323SupportH245Tunnelling OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Support H245 Tunnelling..."
      ::= { voipH323ProtocolOptions 2 }

voipH323SupportEarlyH245 OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Support Early H245..."
      ::= { voipH323ProtocolOptions 3 }


-- Gatekeeper options
voipH323GatekeeperDiscovery OBJECT-TYPE
    SYNTAX INTEGER {
      enabled(1),
      disabled(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "If enabled use GRQ messages to discover location and identity of 
    gatekeepers"
      ::= { voipH323GatekeeperOptions 1 }

voipH323SupportAlternateGatekeeper OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "If true enable the use of alternate gatekeeper information provided in
    response to registration requests"
      ::= { voipH323GatekeeperOptions 2 }

voipH323GatekeeperFullRRQEnable OBJECT-TYPE
    SYNTAX INTEGER {
      enabled(1),
      disabled(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "If enabled, use full RRQ messages for keepalive functions"
      ::= { voipH323GatekeeperOptions 3 }

voipH323GatekeeperIncludeURLID OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..32))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Include optional URL_ID element in registration requests"
      ::= { voipH323GatekeeperOptions 4 }


-- Security options
voipH323SecurityH235Mode OBJECT-TYPE
    SYNTAX INTEGER {
      none(0),
      setup(1),
      ras(2),
      rasArq(3),
      setupArq(4)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the use of H235 protection on various messages. This can range
    from no protection, to protection on all types of messages used for call
    setup"
      ::= { voipH323SecurityOptions 1 }

voipH323SecurityH235Key OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..128))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the key to be used for H235 protection. This consists of a
    16-octet string"
      ::= { voipH323SecurityOptions 2 }


-- MWI options
voipH323SupportMessageWaitingIndicationBlink OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "If true, phone LED will blink when messages are waiting. If false, phone
     LED will not blink even if messages are waiting."
      ::= { voipH323MessageWaitingOptions 1 }

voipH323SupportMessageWaitingIndicationTone OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "If true, dialtone will be replaced with message waiting tone when messages 
    are waiting."
      ::= { voipH323MessageWaitingOptions 2 }


-- Call parking options
voipH323CallParkingTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VoipH323CallParkingEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "Each row of this table controls the POTS line test operation"
      ::= { voipH323CallParkingOptions 1 }

voipH323CallParkingEntry OBJECT-TYPE
    SYNTAX VoipH323CallParkingEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "One row of the table."
    INDEX { voipIfIndex }
      ::= { voipH323CallParkingTable 1 }

VoipH323CallParkingEntry ::= SEQUENCE {
    voipH323CallParkingIndex Integer32,
    voipH323CallParkingAlertEnable TruthValue,
    voipH323CallParkingNumber OCTET STRING
}

voipH323CallParkingIndex OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "A unique index value for the voipH323CallParkingTable table. Each row
    defines the call parking behaviour"
      ::= { voipH323CallParkingEntry 1 }

voipH323CallParkingAlertEnable OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "If true an alert will be generated when a call has been parked"
      ::= { voipH323CallParkingEntry 2 }

voipH323CallParkingNumber OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..32))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The number to which calls should be parked if call parking is enabled"
      ::= { voipH323CallParkingEntry 3 }


