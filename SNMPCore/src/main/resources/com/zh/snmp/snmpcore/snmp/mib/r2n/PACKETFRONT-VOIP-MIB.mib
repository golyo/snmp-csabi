    STATUS current
    DESCRIPTION
    "The maximum delay in seconds before the endpoint will issue an RSIP command"
      ::= { voipMGCPOptions 2 }

voipMGCPSupportPiggyback OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Controls which piggybacking messages is supported"
      ::= { voipMGCPOptions 3 }

voipMGCPSquelchDTMF OBJECT-TYPE
    SYNTAX INTEGER {
      enabled(1),
      disabled(2)
