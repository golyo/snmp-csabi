-- *****************************************************************************
-- All VoIP notifications are defined here.  These can be sent as traps or as 
-- informs.
--
-- *****************************************************************************

notifyVoipRegistration  NOTIFICATION-TYPE
    OBJECTS {
      productMacAddress,
      productIdentity,
      voipIfAuthUser,
      voipIfLineNumber,
      voipIfRegistrationResult
    }
    STATUS      current
    DESCRIPTION
    "The result of the registration procedure is reported to the management system."
      ::= { drgVoipNotifications 1 }

notifyVoipCallQuality NOTIFICATION-TYPE
    OBJECTS {
      productMacAddress,
      productIdentity,
      voipIfAuthUser,
      voipIfLineNumber,
      voipCallQualityJitter,
      voipCallQualityLatency,
      voipCallQualityPacketsLost,
      voipCallQualityPacketsTotal,
      voipCallQualityDuration,
      voipCallQualityAPartyNumber,
      voipCallQualityAPartyName,
      voipCallQualityBPartyNumber,
      voipCallQualityBPartyName,
      voipCallQualityAPartyAddress,
      voipCallQualityBPartyAddress
    }
    STATUS      current
    DESCRIPTION
    "VoIP call quality statistics are reported to the management system."
      ::= { drgVoipNotifications 2 }


