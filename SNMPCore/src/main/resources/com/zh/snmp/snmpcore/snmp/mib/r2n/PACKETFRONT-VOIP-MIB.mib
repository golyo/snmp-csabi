voipQosSignalingPriorityTag OBJECT-TYPE
    SYNTAX Integer32 (0..7)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The IEEE802.1p VLAN priority tag used for call signaling packets"
      ::= { voipQos 3 }

voipQosRtpDSCPTag OBJECT-TYPE 
    SYNTAX Integer32 (0..63)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The DSCP tag used for call media packets"
      ::= { voipQos 4 }

voipQosRtpVlanId OBJECT-TYPE 
    SYNTAX Integer32 (1..4094)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The IEEE802.1Q VLAN id tag used for call media packets"
      ::= { voipQos 5 }

voipQosRtpPriorityTag OBJECT-TYPE
    SYNTAX Integer32 (0..7)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The IEEE802.1p VLAN priority tag used for call media packets"
      ::= { voipQos 6 }



-- *****************************************************************************
-- Jitter Buffer object
--
-- *****************************************************************************

voipJitterBuffer OBJECT IDENTIFIER ::= { drgVoipObjects 3 }

voipJitterBufferMode OBJECT-TYPE
    SYNTAX INTEGER {
      adaptive(1),
      fixed(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Jitter buffer mode - adaptive(1) uses an adaptive size algorithm;
    generally this is the mode which offers the best performance.
    fixed(2) uses a fixed length buffer, this minimises delay."
      ::= { voipJitterBuffer 1 }

voipJitterBufferAutoSwitchEnabled OBJECT-TYPE  
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The use of an adaptive jitter buffer when sending faxes can cause poor
    performance, therefore it is prefereable to use fixed jitter buffer mode
    in this case. This object controls whether the mode will automatically
    switch to fixed mode when a fax transmission is detected"
      ::= { voipJitterBuffer 2 }

voipJitterBufferAdaptiveLength OBJECT-TYPE  
    SYNTAX Integer32 (40..300)
    UNITS "milliseconds"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The maximum length of the adapative jitter buffer in milliseconds"
      ::= { voipJitterBuffer 3 }

voipJitterBufferFixedLength OBJECT-TYPE
    SYNTAX Integer32 (40..120)
    UNITS "milliseconds"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The maximum length of the fixed jitter buffer in milliseconds"
      ::= { voipJitterBuffer 4 }


-- *****************************************************************************
-- Table to describe codec entries
--
-- *****************************************************************************

voipCodec OBJECT IDENTIFIER ::= { drgVoipObjects 4 }

voipCodecEntries OBJECT-TYPE
    SYNTAX Integer32
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The number of codecs present within the device"
      ::= { voipCodec 1 }

voipCodecTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VoipCodecEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "A table with information about available codecs."
      ::= { voipCodec 2 }

voipCodecEntry OBJECT-TYPE
    SYNTAX VoipCodecEntry 
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "One row of the table."
    INDEX { voipIfIndex, voipCodecIndex }
      ::= { voipCodecTable 1 }

VoipCodecEntry ::= SEQUENCE {
    voipCodecLineNumber Integer32,
    voipCodecIndex Integer32,
    voipCodecEnabled TruthValue,
    voipCodecPriority Integer32,
    voipCodecType INTEGER,
    voipCodecPacketizationPeriod Integer32,
    voipCodecSilenceSuppressionEnabled TruthValue,
    voipCodecEchoCancellationEnabled TruthValue,
    voipCodecKeypadType INTEGER
}

voipCodecLineNumber OBJECT-TYPE
    SYNTAX Integer32 (1..4)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "Unique value greater than zero, for line number in each table row. "
      ::= { voipCodecEntry 1 }

voipCodecIndex OBJECT-TYPE
    SYNTAX Integer32 (1..12)
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "Unique value greater than zero, for codec type in each table row. "
      ::= { voipCodecEntry 2 }

voipCodecEnabled OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "true if the codec entry is enabled, i.e. whether this code will be
    included in the list of possible codecs"
      ::= { voipCodecEntry 3 }

voipCodecPriority OBJECT-TYPE
    SYNTAX Integer32 (1..8)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The priority of this codec entry - priority 1 is the highest, lowest
    priority will be equal to the number of codec entries present"
      ::= { voipCodecEntry 4 }

voipCodecType OBJECT-TYPE
    SYNTAX INTEGER {
      g711a(1),
      g711u(2),
      g723(3),
      g729(4)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The type of codec defined for this entry"
      ::= { voipCodecEntry 5 }

voipCodecPacketizationPeriod OBJECT-TYPE
    SYNTAX Integer32 (10..90)
    UNITS "milliseconds"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Voice packet size in milliseconds - a mulitple of 10ms must be used"
      ::= { voipCodecEntry 6 }

voipCodecSilenceSuppressionEnabled OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Controls the generation of silence suppression frames during periods
    of silence. If true, silence suppression frames will be generated, thus
    reducing bandwidth usage"
      ::= { voipCodecEntry 7 }

voipCodecEchoCancellationEnabled OBJECT-TYPE
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Controls the use of the line and acoustic echo canceller (AEC). If true
    echo cancellation is enabled. Note that AEC is automatically disabled when
    fax or modem tones are detected."
      ::= { voipCodecEntry 8 }

voipCodecKeypadType OBJECT-TYPE
    SYNTAX INTEGER {
      none(1),
      rfc2833(2),
      sipInfo(3),	-- SIP specific
      h225(4),		-- H323 specific
      h245(5)		-- H323 specific
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The type of keypad support, i.e. whether telephone events are sent 
    using an inband method, or an out-of-band method. Some choices are 
    protocol specific"
      ::= { voipCodecEntry 9 }


-- *****************************************************************************
-- Table which describes each endpoint instance, i.e. a logical telephone
-- port
--
-- *****************************************************************************

voipIf OBJECT IDENTIFIER ::= { drgVoipObjects 5 }

voipIfTable OBJECT-TYPE
    SYNTAX SEQUENCE OF VoipIfEntry
    MAX-ACCESS   not-accessible
    STATUS       current
    DESCRIPTION
    "A table which describes the parameters required for a line to be unique"
      ::= { voipIf 1 }

voipIfEntry OBJECT-TYPE
    SYNTAX VoipIfEntry 
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "One row of the table."
    INDEX { voipIfIndex }
      ::= { voipIfTable 1 }

VoipIfEntry ::= SEQUENCE {
    voipIfIndex InterfaceIndex,
    voipIfAdmin INTEGER,
    voipIfLineNumber OCTET STRING,
    -- Access control
    voipIfAuthUser OCTET STRING,
    voipIfAuthPasswd OCTET STRING,
    -- Caller ID
    voipIfCallerIdEnabled INTEGER,
    voipIfCallerIdName OCTET STRING,
    -- Call server
    voipIfPrimaryServerAddress OCTET STRING,
    voipIfPrimaryServerPort  Integer32,
    voipIfSecondaryServerAddress OCTET STRING,
    voipIfSecondaryServerPort  Integer32,
    voipIfLocalSignalPort Integer32,
    voipIfHighAvailabilityServerMode INTEGER,
    -- Call server access
    voipIfDomain OCTET STRING,
    voipIfSignalTransportProtocol INTEGER,
    voipIfKeepaliveTimeout Integer32,
    -- Services control
    voipIfFaxAdmin INTEGER,
    voipIfMsgWaitingAccount OCTET STRING,
    -- Signals
    voipIfRingSignal Integer32,
    -- Status information
    voipIfRegistrationResult INTEGER
--    voipIfStatus INTEGER
  }


voipIfIndex OBJECT-TYPE
    SYNTAX InterfaceIndex
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "A unique index value, greater than zero, for each VoIP interface."
      ::= { voipIfEntry 1 }


voipIfAdmin OBJECT-TYPE
    SYNTAX INTEGER {
      enabled(1),
      disabled(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Setting the object to disabled causes the line to be disabled. The line is
    not registered when in this state, and it is not possible to make or
    receive calls."
      ::= { voipIfEntry 2 }


voipIfLineNumber OBJECT-TYPE 
    SYNTAX OCTET STRING  (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The E.164 telephone number assigned to this interface."
      ::= { voipIfEntry 3 }


voipIfAuthUser OBJECT-TYPE
    SYNTAX OCTET STRING  (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The user identifier used to identify the endpoint to the call server"
      ::= { voipIfEntry 4 }

voipIfAuthPasswd OBJECT-TYPE
    SYNTAX OCTET STRING  (SIZE(0..64))
    MAX-ACCESS read-write  -- Note that reading this would allow a hacker to get enough information to steal the users identity
    STATUS current
    DESCRIPTION
    "The password associated with this VoIP user. Note that since reading this
    object would potentially allow unauthorised access to telephony resources,
    reads return an SHA1 one-way hash of the password. An authorised person
    who knows the password should perform an SHA1 hash and compare the result to
    the read object value to verify correctness"
      ::= { voipIfEntry 5 }

voipIfCallerIdEnabled OBJECT-TYPE 
    SYNTAX INTEGER {
      enabled(1),
      disabled(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Controls the generation of caller line identification presentation (CLIP)
    information via the line interface. When enabled, CLIP information will be
    sent to the telephone if a valid caller id is received in the call setup"
      ::= { voipIfEntry 6 }

voipIfCallerIdName OBJECT-TYPE 
    SYNTAX OCTET STRING  (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the name sent in the call setup information when making a call"
      ::= { voipIfEntry 7 }

voipIfPrimaryServerAddress OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..255))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Primary IP addresses and/or FQDNs of telephony server 
    (Gatekeeper, SIP server, MGC) to be used for establishing calls"
      ::= { voipIfEntry 8 }

voipIfPrimaryServerPort OBJECT-TYPE
    SYNTAX Integer32 (1024..65535)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Primary destination ports used for communication with 
    the telephony servers."
      ::= { voipIfEntry 9 }

voipIfSecondaryServerAddress OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..255))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Secondary IP addresses and/or FQDNs of telephony server 
    (Gatekeeper, SIP server, MGC) to be used for establishing calls"
      ::= { voipIfEntry 10 }

voipIfSecondaryServerPort OBJECT-TYPE
    SYNTAX Integer32 (1024..65535)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Secondary destination ports used for communication with 
    the telephony servers."
      ::= { voipIfEntry 11 }

voipIfLocalSignalPort OBJECT-TYPE
    SYNTAX Integer32 (1..65535)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The local port used to receive messages from the telephony server."
      ::= { voipIfEntry 12 }

voipIfHighAvailabilityServerMode OBJECT-TYPE
    SYNTAX INTEGER {                       
      off(1),
      automatic(2),
      fixed(3),
      ms(4)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "off - no secondary telephony server is available.
    automatic - the IP-address of the secondary telephony server
    is configured automatically by the signaling protocol.
    fixed - the IP-address of the secondary telephony server is 
    configured by the operator ( see voipIfSecTelServerAddr )."
      ::= { voipIfEntry 13 }

voipIfDomain  OBJECT-TYPE 
    SYNTAX OCTET STRING  (SIZE(0..255))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The name of the domain that is used for the telephony application. 
    Used in registration as voipIfLineNumber@voipIfDomain.
    URL_ID option in H323, endpoint domain in MGCP"
      ::= { voipIfEntry 14 }

voipIfSignalTransportProtocol OBJECT-TYPE
    SYNTAX INTEGER {
      udp(1),
      tcp(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Controls the transport protocol that is used for the telephony signaling
    protocols."
      ::= { voipIfEntry 15 }

voipIfKeepaliveTimeout OBJECT-TYPE
   SYNTAX Integer32
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION
      "The time the system waits until it refreshes the server registration."
   ::= { voipIfEntry 16 }

voipIfFaxAdmin OBJECT-TYPE
    SYNTAX INTEGER {
      enabled(1),
      disabled(2)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "When enabled the line is able to send and receive fax messages."
      ::= { voipIfEntry 17 }

voipIfMsgWaitingAccount OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Message waiting account username"
      ::= { voipIfEntry 18 }

voipIfRingSignal OBJECT-TYPE
    SYNTAX Integer32 (0..9)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The ringsignal number that will be used to alert the subscriber of an incoming call
     on this telephony line."
      ::= { voipIfEntry 19 }

voipIfRegistrationResult OBJECT-TYPE 
    SYNTAX INTEGER {                
      notAttempted(1),
      success(2),
      rejected(3),
      redirected(4),
      failed(5)
    }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The result from the latest registration sequence."
      ::= { voipIfEntry 20 }


-- *****************************************************************************
-- Line processing
--
-- *****************************************************************************

voipLineConfig OBJECT IDENTIFIER ::= {drgVoipObjects 6 }

voipLineTransmitGain OBJECT-TYPE
    SYNTAX Integer32 (-640..60)
    UNITS "0.1 dB"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Transmit gain in 0.1dB steps. This is the gain from the analog terminal
     towards the IP network. Note that inserting a large attenuation may prevent
     DTMF detection for dialing operating correctly."
      ::= { voipLineConfig 1 }

voipLineReceiveGain OBJECT-TYPE
    SYNTAX Integer32 (-640..60)
    UNITS "0.1 dB"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Receive gain in 0.1dB steps. This is the gain from the IP network to the
     analog terminal"
      ::= { voipLineConfig 2 }

voipLineRingVoltage OBJECT-TYPE
    SYNTAX Integer32 (15..94)
    UNITS "Vrms"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Required ring voltage"
      ::= { voipLineConfig 3 }

voipLineRingFrequency OBJECT-TYPE
    SYNTAX Integer32 (16..50)
    UNITS "Hertz"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Required ring frequency in Hz. Note that only frequencies of 16, 20, 25
    and 50Hz are supported. Other values will be ignored"
      ::= { voipLineConfig 4 }

voipLineLoopCurrentLimit OBJECT-TYPE
    SYNTAX Integer32 (20..41)
    UNITS "mA"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the maximum current used when driving the analog terminal loop."
      ::= { voipLineConfig 5 }


voipLineIfStatus OBJECT IDENTIFIER ::= {drgVoipObjects 7 }

voipLineIfStatusTable OBJECT-TYPE
   SYNTAX SEQUENCE OF VoipLineIfStatusEntry
   MAX-ACCESS   not-accessible
   STATUS       current
   DESCRIPTION
      "A table with information about the status of FXS line interface."
   ::= { voipLineIfStatus 1 }

voipLineIfStatusEntry OBJECT-TYPE
    SYNTAX VoipLineIfStatusEntry 
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION
    "One row of the table."
    INDEX { voipIfIndex }
      ::= { voipLineIfStatusTable 1 }

VoipLineIfStatusEntry ::= SEQUENCE {
    voipLineIfStatusLineNumber Integer32,
    voipLineIfLineStatus INTEGER
}

voipLineIfStatusLineNumber OBJECT-TYPE
    SYNTAX Integer32 (1..4)
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "A unique value, greater than zero, for each table row. "
      ::= { voipLineIfStatusEntry 1 }

voipLineIfLineStatus OBJECT-TYPE
    SYNTAX INTEGER {
      onHook(1),
      offHook(2),
      ringing(3)
    }
    MAX-ACCESS read-only
    STATUS current
    DESCRIPTION
    "The current line status."
      ::= { voipLineIfStatusEntry 2 }


-- *****************************************************************************
-- Tone generator configuration
--
-- *****************************************************************************

voipTones OBJECT IDENTIFIER ::= { drgVoipObjects 8 }

voipCallProgressTones OBJECT IDENTIFIER ::= { voipTones 1 }
voipRingTones OBJECT IDENTIFIER ::= { voipTones 2 }
voipCustomTones OBJECT IDENTIFIER ::= { voipTones 3 }
voipRingCadences OBJECT IDENTIFIER ::= { voipTones 4 }


-- *****************************************************************************
-- Call progress tone definitions
--
-- *****************************************************************************

voipCallProgressDialtone OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the dialtone call progress tone frequency, level, patten and mode."
      ::= { voipCallProgressTones 1 }

voipCallProgressBusy OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the busy call progress tone frequency, level, patten and mode."
      ::= { voipCallProgressTones 2 }

voipCallProgressRingback OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the ringback call progress tone frequency, level, patten and mode."
      ::= { voipCallProgressTones 3 }

voipCallProgressCallWaiting OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the call waiting call progress tone frequency, level, patten and mode."
      ::= { voipCallProgressTones 4 }

voipCallProgressNetworkBusy OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the network busy call progress tone frequency, level, patten and mode."
      ::= { voipCallProgressTones 5 }

voipCallProgressCallForward OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the call forward call progress tone frequency, level, patten and mode."
      ::= { voipCallProgressTones 6 }

voipCallProgressConfirm OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the confirm call progress tone frequency, level, patten and mode."
      ::= { voipCallProgressTones 7 }

voipCallProgressStutterDial OBJECT-TYPE
    SYNTAX OCTET STRING (SIZE(0..64))
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the stutter dial call progress tone frequency, level, patten and mode."
      ::= { voipCallProgressTones 8 }

voipCallProgressOffHookWarning OBJECT-TYPE
