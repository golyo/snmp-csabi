-- *****************************************************************************
-- Global information for the telephony applications in DRGs
--
-- *****************************************************************************

drgVoipNotifications	OBJECT IDENTIFIER ::= { pfVoipMib 0 }
drgVoipObjects		OBJECT IDENTIFIER ::= { pfVoipMib 1 }
drgVoipConformance	OBJECT IDENTIFIER ::= { pfVoipMib 2 }


-- *****************************************************************************
-- Base
--
-- *****************************************************************************

voipConfigBase        OBJECT IDENTIFIER       ::= { drgVoipObjects 1 }

voipIfCount OBJECT-TYPE
     SYNTAX Integer32
     MAX-ACCESS read-only
     STATUS current
     DESCRIPTION
     "The number of IP telephony interfaces in this device."
       ::= { voipConfigBase 1 }

voipDialTimeout OBJECT-TYPE 
    SYNTAX Integer32
    UNITS "seconds"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The time the system waits for another digit to be pressed before the call 
    is sent to the call controller."
      ::= { voipConfigBase 2 }

voipRegResultNotify OBJECT-TYPE
    SYNTAX INTEGER {
      disabled(1),
      always(2),
      onlyErrors(3)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Setting the object to always(2) causes the system to send
    notifyVoipRegistration after every registration procedure
    no matter of result.

    Setting the object to onlyErrors(3) cause the system to
    send NotifyVoipRegistration when an error occurs during
    the registration procedure.

    Setting the object to disabled(1) cause the system to
    not send any notifyVoipRegistration."
      ::= { voipConfigBase 3 }

voipDialPlan OBJECT-TYPE 
   SYNTAX OCTET STRING  (SIZE(0..255))
   MAX-ACCESS read-write
   STATUS current
   DESCRIPTION
    "The calling dialplan - describes what are valid dial patterns and
    allow the call signalling protocols to know when they can send the
    dialled digits to the call server"
   ::= { voipConfigBase 4 }

voipRtpStartPort OBJECT-TYPE
    SYNTAX Integer32 (1024..65535)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The start of the port range for sending RTP media."
      ::= { voipConfigBase 5 }

voipRtpEndPort OBJECT-TYPE
    SYNTAX Integer32 (1024..65535)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The end of the port range for sending RTP media."
      ::= { voipConfigBase 6 }

voipCountry OBJECT-TYPE 
    SYNTAX INTEGER {     
      sweden(1),
      uk(2),
      belgium(3),
      germany(4),
      japan(5),
      us(6),
      denmark(7),
      france(8),
      italy(9),
      china(10),
      venezuala(11),
      norway(12),
      finland(13),
      netherlands(14),
      etsiDuringRinging(15),
      etsiPriorDtAs(16),
      etsiPriorRpAs(17),
      etsiPriorLrPlusDtAs(18),
      bellcore(19)
    }
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Defines the termination impedance and the CLIP standard to be used"
      ::= { voipConfigBase 7 }

voipQuickDialEnabled OBJECT-TYPE 
    SYNTAX TruthValue
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "Control the use of the # key to instigate quick dial function. If enabled
    and the '#' key is pressed, the call setup message will be sent immediately,
    i.e. no wait for dial timeout is used"
      ::= { voipConfigBase 8 }

voipHookflashTimerMin OBJECT-TYPE
    SYNTAX Integer32 (0..800)
    UNITS "milliseconds"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The minimum time the hook switch must be activated for an hookflash event
    to be possible"
      ::= { voipConfigBase 9 }

voipHookflashTimerMax OBJECT-TYPE
    SYNTAX Integer32 (0..800)
    UNITS "milliseconds"
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The maximum time the hook switch must be activated for an hookflash event
    to be possible"
      ::= { voipConfigBase 10 }

voipCodecKeypadPayloadType OBJECT-TYPE
    SYNTAX Integer32 (96..127)
    MAX-ACCESS read-write
    STATUS current
    DESCRIPTION
    "The RTP payload type used when using RFC2833 to send out-of-band
    telephone events"
      ::= { voipConfigBase 11 }


-- *****************************************************************************
-- Quality of Service
--
